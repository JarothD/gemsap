{
  "name": "gemsap",
  "description": "Creador de Certificados",
  "author": "Jarothi",
  "version": "0.7.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.26.10",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.26.9",
    "app-module-path": "^2.2.0",
    "axios": "^1.2.3",
    "bufferutil": "^4.0.7",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "docx": "^7.8.2",
    "easy-template-x": "^3.0.4",
    "electron-is-dev": "2.0.0",
    "electron-log": "^4.4.8",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "libreoffice-convert": "^1.4.1",
    "nodemon": "^2.0.20",
    "pdf-lib": "^1.17.1",
    "piping": "^1.0.0-rc.4",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "react-svg": "^16.1.34",
    "sweetalert2": "^11.7.0",
    "update-electron-app": "^2.0.1",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.12.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.24.0",
    "concurrently": "7.6.0",
    "electron": "22.0.3",
    "electron-builder": "^20.41.0",
    "eslint": "^9.24.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^2.8.3",
    "react-devtools": "^6.1.1",
    "react-scripts": "5.0.1",
    "wait-on": "7.0.1"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./main.js",
      "./preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "react-start": "cross-env BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js",
    "dev": "yarn clean && concurrently \"cross-env BROWSER=none react-scripts start\" \"electron .\"",
    "electron-pack": "yarn electron-builder -c.extraMetadata.main='./main.js'",
    "preelectron-pack": "yarn react-build && yarn server-build",
    "build": "yarn preelectron-pack && yarn electron-pack",
    "clean": "node scripts/clean-ports.js",
    "start": "yarn dev",
    "prod": "electron ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
