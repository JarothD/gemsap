{
  "name": "gemsap",
  "description": "Creador de Certificados",
  "author": "Jarothi",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "app-module-path": "^2.2.0",
    "axios": "^1.2.3",
    "bufferutil": "^4.0.7",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "docx": "^7.8.2",
    "easy-template-x": "^3.0.4",
    "electron-is-dev": "2.0.0",
    "electron-log": "^4.4.8",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "libreoffice-convert": "^1.4.1",
    "nodemon": "^2.0.20",
    "piping": "^1.0.0-rc.4",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "sweetalert2": "^11.7.0",
    "update-electron-app": "^2.0.1",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.12.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "7.6.0",
    "electron": "22.0.3",
    "electron-builder": "^20.41.0",
    "prettier": "^2.8.3",
    "react-scripts": "5.0.1",
    "wait-on": "7.0.1"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./main.js",
      "./preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "yarn electron-builder -c.extraMetadata.main='./main.js'",
    "preelectron-pack": "yarn react-build && yarn server-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"yarn server-start\" \"wait-on http://localhost:3000 && yarn electron-dev \""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
